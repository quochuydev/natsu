{
  "name": "natsu",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "node config-npm-token.js",
    "prepublish": "node config-npm-token.js",
    "dev:enter": "node ./enter-dev-mode.js",
    "dev:exit": "del **/node_modules/@silenteer && yarn install:all",
    "install:all": "node ./install-all.js",
    "uninstall:all": "del **/node_modules **/yarn.lock",
    "test:all": "node ./test-all.js",
    "release": "node ./prepare-release.js && (cd $PROJECT && npx np --branch=main --no-release-draft) && (cd $PROJECT && rm -rf .npmrc)",
    "lint": "eslint --fix --ext .ts,.tsx,.json -c .eslintrc.js ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "concurrently": "6.2.1",
    "del-cli": "4.0.1",
    "dotenv": "10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "jest": "27.0.3",
    "prettier": "2.3.1",
    "typescript": "4.4.3"
  },
  "dependencies": {}
}
